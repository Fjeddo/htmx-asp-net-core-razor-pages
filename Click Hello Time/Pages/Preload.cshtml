@page
@model PreloadModel

<script src="https://unpkg.com/htmx.org@2.0.4"></script>
<script src="https://unpkg.com/htmx-ext-preload@2.1.0"></script>

<div class="text-center">
    <h1 class="display-4" hx-get="@Url.Page("Preload", "Orebro")" hx-swap="innerHTML">Hello Örebro</h1>

    <h1 class="display-4" hx-get="@Url.Page("Preload", "Stockholm")" hx-swap="innerHTML">Hello Stockholm</h1>
</div>

<div class="text-center" hx-ext="preload">
    <div preload>
        <h1 class="display-4" hx-get="@Url.Page("Preload", "Orebro")" hx-swap="innerHTML">Hello Örebro</h1>

        <h1 class="display-4" hx-get="@Url.Page("Preload", "Stockholm")" hx-swap="innerHTML">Hello Stockholm</h1>
    </div>
</div>